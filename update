svn update

if [ -d flotsam_XmlRpcGroup ]; then
	svn update flotsam_XmlRpcGroup
else
	svn co http://www.nsl.tuis.ac.jp/svn/opensim/flotsam_XmlRpcGroup/trunk flotsam_XmlRpcGroup
fi

